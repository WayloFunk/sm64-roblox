{
    "name": "Super Mario 64",

    "tree": {
        "$className": "DataModel",
        "$ignoreUnknownInstances": false,

        "Workspace": {
            "$className": "Workspace",
            "$ignoreUnknownInstances": false,

            "SM64": {
                "$className": "Folder",

                "$attributes": {
                    "TimeScale": 1,
                    "WingCap": false,
                    "HotLoading": false,
                    "DebugToggle": true,
                    "TruncateBounds": true,
                    "ToolAssistedInput": true
                },

                "Client": {
                    "$path": "client"
                },

                "Server": {
                    "$path": "server"
                },

                "Shared": {
                    "$path": "shared"
                }
            }
        }
    }
}